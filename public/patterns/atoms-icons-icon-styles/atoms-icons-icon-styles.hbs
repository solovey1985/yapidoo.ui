{{#each groups as |group| }}
<div>
    <h2>{{ group.name }}</h2>
    <ul class="icon-grid">

        {{#each group.icons as |icon| }}
        <li class="icon-grid__item">
            {{> atoms-icon }}
            <span class="icon-grid__label">{{ icon.icon }}</span>
        </li>
        {{/each}}

    </ul>
</div>
{{/each}}